{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": ["es2018"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../dist/functions",
    "rootDir": ".",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
